<script>
//Promo close
function PromoCloseModal() {
  jQuery('.modal_box_header').css('display','block');
  jQuery('.modal_box').css('background','#ebebeb');
  jQuery('.modal_box').css('position','static');
  jQuery('.modal_box').css('box-shadow','5px 5px 5px rgba(0,0,0,0.5)');
  jQuery('.modal_box_content').css('max-height','250px');
  jQuery('#modal_content_in').html('');
  jQuery('.modal_container').removeClass('modal_open');
  ga('send', 'event', 'Auto Show Popup', 'Closed', 'Click for Offer');
}
//Sitewide modal
function openModal(h,c) {
  jQuery('#modal_header_in').html(h);
  jQuery('#modal_content_in').html(c);
  jQuery('.modal_container').addClass('modal_open');
}
function closeModal() {
  jQuery('#modal_header_in').html('');
  jQuery('#modal_content_in').html('');
  jQuery('.modal_container').removeClass('modal_open');
}
jQuery('.modal_box_close').click(function() {
    closeModal();
    console.log('closing modal');
});
</script>
<!-- Google Optimization Implimentations -->
<script>
(function () {
  try {
    /*-----------------------
      section data 
    -----------------------*/
    var FeHelper = this;
    //wait for element to load
    FeHelper.doWhenElementLoaded = function (
      selector,
      trigger,
      delayInterval,
      delayTimeout
    ) {
      var interval = setInterval(function () {
        if (
          document &&
          document.querySelectorAll(selector) &&
          document.querySelectorAll(selector).length > 0
        ) {
          clearInterval(interval);
          trigger();
        }
      }, delayInterval);
      setTimeout(function () {
        clearInterval(interval);
      }, delayTimeout);
    };
    /*-----------------------
      init is the main controller
      --------------------*/
    FeHelper.add_number = function(){
        var phone_number = document.querySelector(".footer_break a").cloneNode(true);
        console.log(phone_number);
        document.querySelector(".bm-call").insertAdjacentElement("beforeend",phone_number);

        //Change text of the span
        var phone_number_text = document.querySelector(".bm-call a span").textContent;
        var final_string = '('+phone_number_text.substr(0,3)+') '+phone_number_text.substr(4,phone_number_text.length);
        
        document.querySelector(".bm-call a span").innerHTML = final_string;

        document.addEventListener('scroll',function(){
            console.log("scrool");
            var navbar = document.querySelector("#bm-wrapper");
            console.log(navbar);
            var sticky = navbar.offsetTop;
            if (window.pageYOffset > sticky) {
                navbar.classList.add("sticky")

                //Now show the img and phone number to the header
                document.querySelectorAll("#bm-wrapper > a img")[1].style.display = "block";
                document.querySelectorAll("#bm-wrapper > a img")[0].style.display = "none";
                document.querySelector("#bm-honda-image").style.display = "none";
                document.querySelector(".bm-call").style.display = "block";
            } else {
                navbar.classList.remove("sticky");
                document.querySelectorAll("#bm-wrapper > a img")[1].style.display = "none";
                document.querySelectorAll("#bm-wrapper > a img")[0].style.display = "block";
                document.querySelector("#bm-honda-image").style.display = "block";
                document.querySelector(".bm-call").style.display = "none";
            }
        })
    }
    FeHelper.mobile = function () {
        //First add an attribute to the parent for css
        document.querySelector("header .dh_mobile .x-container").setAttribute("id","bm-wrapper");

        //Add new image to the 7 year section
        var html = ''+
        '<img id="bm-dick-hannah" src="https://bm-test-dev.s3.us-east-2.amazonaws.com/DickHannah/DHD%2B70yr_lock-up_440x80.png" alt="Dick Hannah Dealerships">';
        document.querySelector("#bm-wrapper > a").insertAdjacentHTML("beforeend",html);

        //Dispaly none for the second image
        document.querySelectorAll("#bm-wrapper > a")[1].setAttribute("id","bm-honda-image");

        //Now add phone number in the honda section
        html = ''+
        '<div class="bm-call">'+
            '<span>Call </span>'+
        '</div>';
        document.querySelector("#bm-wrapper > a").insertAdjacentHTML("afterend",html);
        
        //Now add the phone number to the bm-call div
        FeHelper.doWhenElementLoaded(".footer_break a", FeHelper.add_number, 50, 10000);
        
    };
    FeHelper.init = function () {
      //Only in the mobile
      var width = window.screen.width;
      if (width < 768) {
        FeHelper.mobile();
      }
    };
    FeHelper.doWhenElementLoaded("header", FeHelper.init, 50, 10000);
  } catch (err) {
    // console && console.log(err);
  }
})();
</script>
