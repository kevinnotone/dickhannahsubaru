function submissionCheck() {
  var ok=true;
    if (jQuery('#_3m4nt5r1f').val()=='') {
        ok=false;
    }
    if (jQuery('#_3m4nt541').val()=='') {
        ok=false;
    }
    var email_val = jQuery('#_1i4m3').val();
    if( ! emailPattern.test(email_val) ){
        ok=false;
    }
  return ok;
}
